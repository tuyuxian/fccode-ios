extend type Mutation {
  createLifePhoto(userId: ID!, input: CreateLifePhotoInput!): CRUDResponse!
  updateLifePhoto(
    userId: ID!
    lifePhotoId: ID!
    input: UpdateLifePhotoInput!
  ): CRUDResponse!
  deleteLifePhoto(userId: ID!, lifePhotoId: ID!, position: Int!): CRUDResponse!
  setMainLifePhoto(
    userId: ID!
    sourceLifePhotoId: ID!
    targetLifePhotoId: ID!
    fromPosition: Int!
  ): CRUDResponse!
}
